; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GoFast"
run/main_scene="res://scenes/levels/world.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[aseprite]

import/import_plugin/default_automatic_importer="Static Texture"

[autoload]

ScoreCounter="*res://scripts/autoloads/score_counter.gd"
Utility="*res://scripts/autoloads/utility.gd"
SignalBus="*res://scripts/autoloads/signal_bus.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://assets/sprites/reticle_01_large.png"
mouse_cursor/custom_image_hotspot=Vector2(80, 80)

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg")
